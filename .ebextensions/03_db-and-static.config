container_commands:
  01_migrate:
    command: "django-admin.py migrate"
    leader_only: true
  02_collectstatic:
    command: "source /opt/python/run/venv/bin/activate && python web/manage.py collectstatic --noinput"
  03_createsu:
    command: "source /opt/python/run/venv/bin/activate && python /opt/python/current/app/web/manage.py createsu"
    leader_only: true
option_settings:
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: "hepcat.settings"
